{% extends 'base.html.twig' %}

{% block title %}Ajouter un systeme d'acquisition (SA){% endblock %}

{% block body %}
    <a class="btn btn-primary margin-10" href="{{ path('app_sa_liste')}}" role="button">Retour</a>

    <div class="container my-5">
        <h1 class="text-center mb-4">Suppression du SA {{ SA.nom }}</h1>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button class="btn">Supprimer</button>
        {{ form_end(form) }}
    </div>
{% endblock %}
